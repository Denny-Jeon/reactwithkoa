{
    "name": "reactwithkoa",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.3",
        "@testing-library/user-event": "^7.1.2",
        "formik": "^2.1.2",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.3.0",
        "recompose": "^0.30.0",
        "yup": "^0.28.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": [
            "airbnb"
        ],
        "plugins": [
            "react",
            "jsx-a11y",
            "import"
        ],
        "parserOptions": {
            "ecmaVersion": 2016,
            "sourceType": "module"
        },
        "env": {
            "es6": true,
            "browser": true,
            "node": true
        },
        "rules": {
            "linebreak-style": 0,
            "quotes": [
                "error",
                "double",
                {
                    "avoidEscape": true
                }
            ],
            "indent": [
                "error",
                2
            ],
            "react/jsx-indent": [
                "error",
                2
            ],
            "react/jsx-indent-props": [
                "error",
                2
            ],
            "react/prefer-stateless-function": 0,
            "react/jsx-one-expression-per-line": 0,
            "react/jsx-filename-extension": [
                1,
                {
                    "extensions": [
                        ".js",
                        ".jsx"
                    ]
                }
            ]
        }
    },
    "devDependencies": {
        "eslint-config-airbnb": "^18.0.1"
    }
}
